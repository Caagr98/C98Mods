{
	"multipart": [
		{
			"when": {"down": true, "up": false},
			"apply": {"model": "c98/magic:xp_pipe/short"}
		},
		{
			"when": {"down": false, "up": true},
			"apply": {"model": "c98/magic:xp_pipe/short", "uvlock": true, "x": 180}
		},
		{
			"when": {"down": true, "up": true},
			"apply": {"model": "c98/magic:xp_pipe/long"}
		},
		{
			"when": {"north": true, "south": false},
			"apply": {"model": "c98/magic:xp_pipe/short", "uvlock": true, "x": 270}
		},
		{
			"when": {"north": false, "south": true},
			"apply": {"model": "c98/magic:xp_pipe/short", "uvlock": true, "x": 90}
		},
		{
			"when": {"north": true, "south": true},
			"apply": {"model": "c98/magic:xp_pipe/long", "uvlock": true, "x": 90}
		},
		{
			"when": {"west": true, "east": false},
			"apply": {"model": "c98/magic:xp_pipe/short", "uvlock": true, "x": 270, "y": 270}
		},
		{
			"when": {"west": false, "east": true},
			"apply": {"model": "c98/magic:xp_pipe/short", "uvlock": true, "x": 90, "y": 270}
		},
		{
			"when": {"west": true, "east": true},
			"apply": {"model": "c98/magic:xp_pipe/long", "uvlock": true, "x": 90, "y": 270}
		},
		{
			"when": {"down": false, "up": false, "north": false, "south": false, "west": false, "east": false},
			"apply": {"model": "c98/magic:xp_pipe/none"}
		},
		{
			"when": {"down": true, "up": false, "north": false, "south": false, "west": false, "east": false},
			"apply": {"model": "c98/magic:xp_pipe/end"}
		},
		{
			"when": {"down": false, "up": true, "north": false, "south": false, "west": false, "east": false},
			"apply": {"model": "c98/magic:xp_pipe/end", "uvlock": true, "x": 180}
		},
		{
			"when": {"down": false, "up": false, "north": true, "south": false, "west": false, "east": false},
			"apply": {"model": "c98/magic:xp_pipe/end", "uvlock": true, "x": 270}
		},
		{
			"when": {"down": false, "up": false, "north": false, "south": true, "west": false, "east": false},
			"apply": {"model": "c98/magic:xp_pipe/end", "uvlock": true, "x": 90}
		},
		{
			"when": {"down": false, "up": false, "north": false, "south": false, "west": true, "east": false},
			"apply": {"model": "c98/magic:xp_pipe/end", "uvlock": true, "x": 270, "y": 270}
		},
		{
			"when": {"down": false, "up": false, "north": false, "south": false, "west": false, "east": true},
			"apply": {"model": "c98/magic:xp_pipe/end", "uvlock": true, "x": 90, "y": 270}
		}
	]
}
